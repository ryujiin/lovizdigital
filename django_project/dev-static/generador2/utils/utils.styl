
.box_contraste
	background #111
	color #fff
	a
		color #fff
.sidebar-overlay
	background-color rgba(255,255,255,.6)
	position fixed
	top 0
	left 0
	width 100%
	height 100%
	z-index 100
	display none
.label
	color #111
nav	.nav-pills
	a
		padding 5px
		&:hover,&.active,&:focus
			background none
			text-decoration underline
		
img
	width 100%
.pull-right
	float right
.overflow
	overflow hidden
p.bg-warning
	padding 10px
.clear_both
	clear both
.enlinea
	display inline-block
.modal-content
	border-radius 0px
	
#ajaxloader_img
	position: absolute;
	width: 30px;
	height: 30px;
	border: 8px solid #fff;
	border-right-color: transparent;
	border-radius: 50%;
	box-shadow: 0 0 25px 2px #eee;
	top 35%
	left 40%
#ajaxloader_img
	animation: spin 1s linear infinite;
ul
	&.no-list
		padding 0
		margin 0
		list-style none
@keyframes spin
{
	from { transform: rotate(0deg);   opacity: 0.2; }
	50%  { transform: rotate(180deg); opacity: 1.0; }
	to   { transform: rotate(360deg); opacity: 0.2; }
}