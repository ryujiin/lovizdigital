a
	color #111
	&:hover
		color #111
.ajax_full
	width 100%
	height 100%	
	position fixed
	top 0
	background rgba(255, 255, 255, 0.9)	
	z-index 2000
	.fondo_ajax
		width 100px
		position absolute
		top 45%
		left 45%
		.ajax
			background-repeat no-repeat
			background-position center
			width 150px
			height 64px
		.img_ajax
			background-image url('../imgs/ajaxloader.gif')			
		.text_ajax
			background-image url('../imgs/letrajax.gif')						
.bloque_sus_up
	display none;
	position fixed
	bottom 0
	background rgba(0,0,0,0.9)
	color #fff
	width 100%
	padding 10px
	z-index 10
	.cerrar
		position absolute
		width 25px
		height 25px
		top -35px
		right 20%
		color #000
		cursor pointer
		span
			border 2px solid #000
			border-radius 20px
			padding 3px
			
	.contenedor
		width 500px
		margin 0 auto
		text-align center
		position relative			
		form
			position relative
			.submit
				position absolute
				padding 6px
				top 0
				right 0
				background #111
				border none
		h2
			font italic 25px Georgia
			text-transform none
.box_contraste
	background #111
	color #fff
	a
		color #fff
.clear_both
	clear both
img
	width 100%
.enlinea
	display inline-block
.label
	color #111

nav	.nav-pills
	a
		padding 5px
		&:hover,&.active,&:focus
			background none
			text-decoration underline
.overflow
	overflow hidden
.pull-right
	float right
p.bg-warning
	padding 10px

.modal-content
	border-radius 0px
.sidebar-overlay
	background-color rgba(255,255,255,.6)
	position fixed
	top 0
	left 0
	width 100%
	height 100%
	z-index 100
	display none
.panel
	border-radius 0px
	.panel-heading
		font-weight 700
		background none
		font-family f_imprenta
		text-transform uppercase
		font-size 1.15em
		letter-spacing 2px
#ajaxloader_img
	position: absolute;
	width: 30px;
	height: 30px;
	border: 8px solid #fff;
	border-right-color: transparent;
	border-radius: 50%;
	box-shadow: 0 0 25px 2px #eee;
	top 35%
	left 40%
#ajaxloader_img
	animation: spin 1s linear infinite;
//Estilos de ayuda para el texto plano
.secNav
	list-style none
	li
		display inline-block
		margin 0 10px
		border-right 1px solid #111
		&:last
			border-right none
ul
	&.no-list
		padding 0
		margin 0
		list-style none
@keyframes spin
{
	from { transform: rotate(0deg);   opacity: 0.2; }
	50%  { transform: rotate(180deg); opacity: 1.0; }
	to   { transform: rotate(360deg); opacity: 0.2; }
}